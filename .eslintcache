nds = friendsList.map((friend,key)=>{\r\n        return (\r\n            <li key={key}>\r\n                {friend.email}\r\n                {friend.username}\r\n                <button>Invite</button>\r\n            </li>\r\n        )\r\n    })\r\n    console.log(friendsList)\r\n    console.log(Context.recipes)\r\n    const event ={\r\n       \r\n           event_date: eventDate\r\n    }\r\n    const handleSave=(e)=>{\r\n\r\n        fetch(`${config.SERVER_ENDPOINT}/events/event/${event_id}`, {\r\n            method: 'Put',\r\n            headers: {\r\n              'content-type': 'application/json'\r\n            },\r\n            body: JSON.stringify(event)\r\n          })\r\n            .then(res => {\r\n              if (!res.ok){\r\n                return res.json().then(e => Promise.reject(e))\r\n              }\r\n              return res.json()\r\n            })\r\n            .then(event => {\r\n              history.push(`/users/${event.host_id}`);\r\n              return event\r\n            })\r\n    }\r\n    return(\r\n        <div className=\"createEvent\">\r\n          <Header home={`/users/${user_id}`} profile={`/users/${user_id}/profile`} events={`/users/${user_id}/events`} list={`/users/${user_id}/list`} />\r\n            \r\n            <SelectRecipeForEvent event_id={event_id} selected={eventRecipe}/>\r\n            <LocalizationProvider dateAdapter={AdapterDateFns}>\r\n              <br/>\r\n      <DateTimePicker\r\n        renderInput={(props) => <TextField {...props} />}\r\n        label=\"DateTimePicker\"\r\n        value={eventDate}\r\n        onChange={(newValue) => {\r\n          setValue(newValue);\r\n        }}\r\n      />\r\n      <button onClick={handleSave}>Save</button>\r\n    </LocalizationProvider>\r\n            <h4>\r\n                Host: {Context.user.username}\r\n            </h4>\r\n            <ul>\r\n                {displayFriends}\r\n            </ul>\r\n        </div>\r\n    )    \r\n}","C:\\Users\\haoxa\\projects\\Coo